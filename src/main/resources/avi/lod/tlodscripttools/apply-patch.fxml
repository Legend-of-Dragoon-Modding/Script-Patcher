<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="body" stylesheets="@../../../css/gen.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="avi.lod.tlodscripttools.ApplyPatch">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="applyPatchButton" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#pickPatch" prefHeight="24.0" prefWidth="80.0" text="Pick">
               <font>
                  <Font name="Inter Regular" size="12.0" />
               </font>
            </Button>
            <Label fx:id="statusLabel" alignment="CENTER" text="You must pick a patch file" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
